.tabs-list {
  margin: 0 1.2rem; }
  .tabs-list h1 {
    color: #5863b8; }
  .tabs-list h2 {
    color: #505792; }
  .tabs-list h3 {
    color: #464c7d; }
  .tabs-list p, .tabs-list h1, .tabs-list h2, .tabs-list h3, .tabs-list a {
    overflow: visible; }

.hugo-logo {
  display: block;
  margin: 7% auto; }
  @media only screen and (max-width: 820px) and (orientation: portrait) {
    .hugo-logo {
      margin: auto; } }
.index-content {
  margin: 5%;
  flex: 0; }

.navbar {
  display: flex;
  justify-content: space-evenly;
  flex-direction: row;
  text-align: center;
  min-height: 1.3rem;
  overflow: hidden;
  background-color: #474747;
